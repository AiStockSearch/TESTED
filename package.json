{
  "name": "haqqexMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "installLocale": "yarn install && npx pod-install && yarn ios:devDebug",
    "test": "jest",
    "lint": "eslint --ext .ts,.js,.jsx,.tsx ./app",
    "start": "npx react-native start --reset-cache",
    "extract": "lingui extract && yarn compile",
    "compile": "lingui compile",
    "corepack": "npm install -g corepack --force &&  corepack enable && yarn install && npx pod-install",
    "translate": "yarn extract && node ./src/locales/locale.js && node ./src/locales/extract.locale.js && yarn compile",
    "android:devDebug": "npx react-native run-android --tasks=installDevDebug && adb reverse tcp:8081 tcp:8081",
    "android:prodDebug": "npx react-native run-android --tasks=installProdDebug",
    "ios:devDebug": "npx react-native run-ios --simulator='iPhone 14 Pro' --scheme \"haqqexMobileDev\"",
    "ios:prodDebug": "npx react-native run-ios --simulator='iPhone 14 Pro Max' --scheme \"haqqexMobile\""
  },
  "dependencies": {
    "@lingui/cli": "^4.7.2",
    "@lingui/format-json": "^4.7.2",
    "@lingui/macro": "^4.7.2",
    "@lingui/react": "^4.7.2",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/drawer": "^6.6.4",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "core-js": "^3.37.1",
    "jetifier": "^2.0.0",
    "ramda": "^0.29.1",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-notifier": "^1.9.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.11.0",
    "react-native-redash": "^18.1.3",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-spacing-system": "^2.0.0-rc.0",
    "react-native-svg": "^15.3.0",
    "react-native-video": "^6.3.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@tsconfig/react-native": "^3.0.0",
    "@types/ramda": "^0.29.12",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/validator": "^13.11.9",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "open-google-translator": "^1.0.4",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.6",
    "reactotron-redux": "^3.1.9",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.8.2+sha512.8cf1a37cdbccd13909ba9299dcb8e772292e76155aeeff14fd0a2488f74852c5345e4a2ca81c86a662d727c6d576f6e5b752b4b0958e8433e2c9869a748daaec"
}
